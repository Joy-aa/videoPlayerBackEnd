<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.newhome.mapper.AlgorithmMapper">
    <select id="findAlgoByName" resultType="org.newhome.entity.Algorithm">
        select * from algorithm
        where algo_name = #{algorithmName}
    </select>
    <select id="searchAlgorithm" resultType="org.newhome.entity.Algorithm">
        select * from algorithm
    </select>
</mapper>
